# interfaces(5) file used by ifup(8) and ifdown(8)
# Include files from /etc/network/interfaces.d:
source /etc/network/interfaces.d/*

# lo is the loopback interface, em1 is the active ethernet interface
auto lo eth0
iface lo inet loopback 
# Configuration for em1 
# This line ensures that the interface will be brought up during boot
auto eth0
allow-hotplug eth0 
# The address, gateway and netmask are necessary. 
iface eth0 inet static 
    address 192.168.9.193/23
    gateway 192.168.9.111
    dns-search rotaspi
    dns-namesevers 192.168.9.147, 1.1.1.1
